<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.ThirdFragment">

    <androidx.appcompat.widget.AppCompatEditText
    android:id="@+id/edit_text_study_address"
    android:hint="Study Address"
    style="@style/Base.EditText"
    android:background="@drawable/edit_text_corners"
    android:layout_gravity="center_horizontal"
    android:layout_marginTop="100dp"
    />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_text_job_address"
        android:hint="Job Address"
        style="@style/Base.EditText"
        android:background="@drawable/edit_text_corners"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="210dp"
        />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_finish"
        style="@style/Base.Button"
        android:text="finish"
        android:layout_gravity="center"
        />

</FrameLayout>